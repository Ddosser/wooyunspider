{
    "npm-modules": {
        "key": "./npm-shrinkwrap.json",
        "cache": "../node_modules",
        "generate": "./uncached-npm-install.sh"
    },
    "rubygems": {
        "key": "$BUNDLE_GEMFILE",
        "cache": "../vendor/cache",
        "generate": "cd .. ; bundle install --path=\"`pwd`/vendor/cache\""
    }
}
